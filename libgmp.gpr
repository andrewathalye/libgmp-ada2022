library project libGMP is
	for Library_Name use "libgmp-ada";
	for Library_Kind use "static";
	for Library_Dir use "lib";
	for Object_Dir use "obj";
	for Source_Dirs use ("src");

	package Compiler is
		for Default_Switches ("Ada") use ("-gnatwa", "-gnatVa", "-gnaty-h", "-O2");
	end Compiler;
end libGMP;
